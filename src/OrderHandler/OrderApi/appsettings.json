{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "producer": {
    "bootstrapservers": "localhost:9092"
  },
  "consumer": {
    "bootstrapservers": "localhost:9092", //specify your kafka broker address
    "groupid": "csharp-consumer",
    "enableautocommit": true,
    "statisticsintervalms": 5000,
    "sessiontimeoutms": 10,
    "autooffsetreset": 0,
    "enablepartitioneof": true
  },
  "AllowedHosts": "*"
}